ddns-update-style none;

log-facility local7;

subnet 192.168.{{user_x}}.0 netmask 255.255.255.0 {
  range 192.168.{{user_x}}.100 192.168.{{user_x}}.200;
  option routers 192.168.{{user_x}}.1;
  option domain-name "corp{{user_x}}.un";
  option domain-name-servers 192.168.{{user_x}}.1;
  default-lease-time 600;
  max-lease-time 7200;
}
