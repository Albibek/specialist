;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.corp{{ user_x }}.un. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.corp{{ user_x }}.un.
ns	IN	A	192.168.{{ user_x }}.1
1	IN	PTR	gate.corp{{ user_x }}.un.
2	IN	PTR	server.corp{{ user_x }}.un.
